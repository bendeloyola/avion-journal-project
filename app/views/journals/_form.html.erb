<%= form_with model: @journal, local: true do |f|%>
  <%= f.label :title%>
  <%= f.text_field :title%>
  <%= f.label :content%>
  <%= f.text_area :content%>
  <%= f.label "Categories"%>
  <%= f.select :category_id, Category.all.map {|c|[c.name, c.id]} %>

  <%= f.submit %>
  <%= link_to "Back", journals_path %>
<% end  %>